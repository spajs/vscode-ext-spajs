{
    "name": "spajs",
    "displayName": "SPAjs",
    "description": "Code Snippets for SPA.js library/framework https://spa.js.org",
    "keywords": [
        "SPAjs",
        "SPA js",
        "SPA.js",
        "Single Page Application"
    ],
    "version": "2.0.0",
    "publisher": "spajs",
    "icon": "images/spajsIcon.png",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/spajs/vscode-ext-spajs.git"
    },
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/spajs.json"
            },
            {
                "language": "html",
                "path": "./snippets/spahtm.json"
            },
            {
                "language": "css",
                "path": "./snippets/spacss.json"
            }
        ]
    }
}