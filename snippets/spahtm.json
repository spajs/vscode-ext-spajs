{
  ".html": {
    "html-spa": {
      "prefix": "!html-spa",
      "body":"<!DOCTYPE html>\n<html lang=\"en-US\">\n\n<head>\n\t<meta charset=\"UTF-8\" />\n\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n\t<title>${1:Single Page App}</title>\n\n</head>\n\n<body>\n\n\t<spa-component src=\"${2:rootComponentName}\"></spa-component>\n$0\n\t<script src=\"${3://cdn.jsdelivr.net/npm/jquery@latest/dist/jquery.min.js}\"></script>\n\t<script src=\"${4://cdn.jsdelivr.net/npm/handlebars@latest/dist/handlebars.min.js}\"></script>\n\t<script src=\"${5://cdn.jsdelivr.net/gh/sucom/SPA.js/dist/spa-bundle.min.js}\"></script>\n\n</body>\n\n</html>",
      "description": "SPA - HTML boilerplate for SPA.js"
    },
    "html-spa-bs": {
      "prefix": "!html-spa+bootstrap",
      "body":"<!DOCTYPE html>\n<html lang=\"en-US\">\n\n<head>\n\t<meta charset=\"UTF-8\" />\n\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n\t<title>${1:Single Page App}</title>\n\n\t<link rel=\"stylesheet\" href=\"${8://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css}\" />\n\n</head>\n\n<body>\n\n\t<spa-component src=\"${2:rootComponentName}\"></spa-component>\n$0\n\t<script src=\"${3://cdn.jsdelivr.net/npm/jquery@latest/dist/jquery.min.js}\"></script>\n\t<script src=\"${4://cdn.jsdelivr.net/npm/popper.js@latest/dist/umd/popper.min.js}\"></script>\n\t<script src=\"${5://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/js/bootstrap.min.js}\"></script>\n\t<script src=\"${6://cdn.jsdelivr.net/npm/handlebars@latest/dist/handlebars.min.js}\"></script>\n\t<script src=\"${7://cdn.jsdelivr.net/gh/sucom/SPA.js/dist/spa-bundle.min.js}\"></script>\n\n</body>\n\n</html>",
      "description": "SPA - HTML boilerplate for SPA.js with Bootstrap"
    },
    "html-spa-component": {
      "prefix": "!template-spa-component",
      "body":"<${1:section} class=\"container-component\" id=\"c_${TM_DIRECTORY/.*components(\\/|\\\\)(([^\\/\\\\]*)(\\/|\\\\)?)|(\\/|\\\\)([^\\/\\\\]*)/$3${4:+_}${6:+_}$6/g}\">\n\t$0\n</${1:section}>",
      "description": "SPA - HTML boilerplate for SPA.js component"
    },

    "cdn-bs-css" : {
      "prefix": "cdn:bs-css",
      "body": "<link rel=\"stylesheet\" href=\"//cdn.jsdelivr.net/npm/bootstrap@${1:4.3.1}/dist/css/bootstrap.min.css\">$0",
      "description": "Bootstrap CSS cdn link[href]"
    },
    "cdn-bs-js" : {
      "prefix": "cdn:bs-js",
      "body": "<script src=\"//cdn.jsdelivr.net/npm/jquery@latest/dist/jquery.min.js\"></script>\n<script src=\"//cdn.jsdelivr.net/npm/popper.js@latest/dist/umd/popper.min.js\"></script>\n<script src=\"//cdn.jsdelivr.net/npm/bootstrap@${1:4.3.1}/dist/js/bootstrap.min.js\"></script>$0",
      "description": "Bootstrap JS cdn script[src]"
    },
    "cdn-spa-js" : {
      "prefix": "cdn:spa-js",
      "body": "<script src=\"//cdn.jsdelivr.net/npm/jquery@latest/dist/jquery.min.js\"></script>\n<script src=\"//cdn.jsdelivr.net/npm/handlebars@latest/dist/handlebars.min.js\"></script>\n<script src=\"//cdn.jsdelivr.net/gh/sucom/SPA.js/dist/spa-bundle.min.js\"></script>$0",
      "description": "SPA JS cdn script[src]"
    },
    "cdn-jq-js" : {
      "prefix": "cdn:jquery-js",
      "body": "<script src=\"//cdn.jsdelivr.net/npm/jquery@${1:latest}/dist/jquery.min.js\"></script>$0",
      "description": "jQuery cdn script[src]"
    },
    "cdn-handlebars-js" : {
      "prefix": "cdn:handlebars-js",
      "body": "<script src=\"//cdn.jsdelivr.net/npm/handlebars@${1:latest}/dist/handlebars.min.js\"></script>$0",
      "description": "Handlebars cdn script[src]"
    },
    "cdn-lodash-js" : {
      "prefix": "cdn:lodash-js",
      "body": "<script src=\"//cdn.jsdelivr.net/npm/lodash@${1:latest}/lodash.min.js\"></script>$0",
      "description": "Lodash cdn script[src]"
    },

    "meta-seo" : {
      "prefix": "meta:seo",
      "body": "<meta name=\"robots\" content=\"index,follow\" />\n<meta name=\"keywords\" content=\"${1}\" />\n<meta name=\"description\" content=\"${2}\" />$0",
      "description": "meta tags for SEO"
    },
    "meta-no-cache" : {
      "prefix": "meta:no-cache",
      "body": "<meta http-equiv=\"Cache-Control\" content=\"no-cache, no-store, must-revalidate\" />\n<meta http-equiv=\"Pragma\" content=\"no-cache\" />\n<meta http-equiv=\"Expires\" content=\"0\" />$0",
      "description": "meta tags for no-cache"
    },
    "meta-fav-icon" : {
      "prefix": "meta:fav-icon",
      "body": "<link rel=\"icon\" type=\"image/${1:png}\" href=\"${2:favicon.png}\" />$0",
      "description": "meta tags for fav-icon"
    },

    "spa-html-template": {
      "prefix": ["spa-template", "-template"],
      "body": "<spa-template src=\"${1:componentName}\"></spa-template>$0",
      "description":"SPA - Load Template Component here"
    },
    "x-html-template": {
      "prefix": ["x-template"],
      "body": "<x-template src=\"${1:componentName}\"></x-template>$0",
      "description":"SPA - Load Template Component here"
    },
    "spa-html-component": {
      "prefix": ["spa$", "spa-component", "-component"],
      "body": "<spa-component src=\"${1:componentName}\"></spa-component>$0",
      "description":"SPA - Render Component here using tag"
    },
    "x-html-component": {
      "prefix": ["x-component"],
      "body": "<x-component src=\"${1:componentName}\"></x-component>$0",
      "description":"SPA - Render Component here using tag"
    },
    "data-spa-component": {
      "prefix": "data-spa-component",
      "body": "data-spa-component=\"${1:componentName}\"",
      "description":"SPA - Render Component here using attribute"
    },

    "i18n-lang": {
      "prefix": "i18n-lang",
      "body": "i18n-lang=\"${1:en_US}\"",
      "description":"SPA - i18n default language in <body> tag"
    },
    "data-i18n": {
      "prefix": "data-i18n",
      "body": "data-i18n=\"${1:i18n.key}\"",
      "description":"SPA - i18n label for key"
    },
    "data-i18n-lang": {
      "prefix": "data-i18n-lang",
      "body": "data-i18n-lang=\"${1:en_US}\"",
      "description":"SPA - i18n for language switch/change"
    },

    "data-spa-route": {
      "prefix": "data-spa-route",
      "body": "data-spa-route=\"${1}\"",
      "description":"SPA - Routing Path"
    },
    "data-spa-route-params": {
      "prefix": "data-spa-route-params",
      "body": "data-spa-route-params=\"${1:param1}:'$0'\"",
      "description":"SPA - Routing Parameters"
    },

    "spa-nav-show": {
      "prefix": "spa-show-nav-class",
      "body": "SHOW-SPA-NAV",
      "description":"SPA - Navigation control using body class to show url routes."
    },
    "spa-nav-hide": {
      "prefix": "spa-hide-nav-class",
      "body": "HIDE-SPA-NAV",
      "description":"SPA - Navigation control using body class to hide url routes."
    },
    "spa-nav-block": {
      "prefix": "spa-block-nav-class",
      "body": "BLOCK-SPA-NAV",
      "description":"SPA - Navigation control using body class to block browser's back/forward navigation."
    },

    "spa-defaults-components": {
      "prefix": ["spa-components-defaults", "-defaults"],
      "body" : "data-spa-defaults-components=\"scriptExt:'.js', templateExt:'.html', rootPath:'app/components/', inFolder:true\"",
      "description": "SPA - component's default attributes in body tag"
    },

    "a-rel" : {
      "prefix": ["a-rel", "a:rel"],
      "body" : "rel=\"noopener noreferrer\"",
      "description": "a [rel]"
    },

    "a-target-blank" : {
      "prefix": ["target:blank", "a:target"],
      "body" : "target=\"_blank\" rel=\"noopener noreferrer\"",
      "description": "a [rel]"
    },

    "hb-spa$": {
      "prefix": ":$",
      "body": "{$${1:componentName}}${2:method}();",
      "description":"SPA - HB-helper {$componentName}method();"
    },

    "hb-consoleClear": {
      "prefix": ":consoleClear",
      "body": "{{:consoleClear }}",
      "description":"SPA - HB-helper console.clear"
    },
    "hb-consoleLog": {
      "prefix": ":consoleLog",
      "body": "{{:consoleLog ${1:variable} }}",
      "description":"SPA - HB-helper console.log"
    },

    "hb-trim": {
      "prefix": ":trim",
      "body": "{{:trim ${1:srcStringVariable} }}",
      "description":"SPA - HB-helper trimStr"
    },
    "hb-trimStr": {
      "prefix": ":trimStr",
      "body": "{{:trimStr ${1:srcStringVariable} '${2:trimString}' }}",
      "description":"SPA - HB-helper trimStr"
    },
    "hb-trimLeft": {
      "prefix": ":trimLeft",
      "body": "{{:trimLeft ${1:srcStringVariable} }}",
      "description":"SPA - HB-helper trimLeft"
    },
    "hb-trimLeftStr": {
      "prefix": ":trimLeftStr",
      "body": "{{:trimLeftStr ${1:srcStringVariable} '${2:trimString}' }}",
      "description":"SPA - HB-helper trimLeftStr"
    },
    "hb-trimRight": {
      "prefix": ":trimRight",
      "body": "{{:trimRight ${1:srcStringVariable} }}",
      "description":"SPA - HB-helper trimRight"
    },
    "hb-trimRightStr": {
